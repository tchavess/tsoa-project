{
  "version": "2.0.0",
  "command": "yarn",
  "type": "shell",
  "tasks": [
    {
      "label": "build",
      "group": "build",
      "args": ["run", "build"]
    },
    {
      "label": "test",
      "group": "test",
      "args": ["test"]
    },
    {
      "label": "prepareFiles",
      "group": "none",
      "args": ["run", "pretest"]
    }
  ]
}
